{
  "compilerOptions": {
      "target": "ES6",
      "lib": ["DOM", "DOM.Iterable", "ES2021"],
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "strictNullChecks": true
  }
}
